/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include "marvell/dove.dtsi"

/ {
	model = "Dell Wyse Tx0 (T10/T50)";
	compatible = "wyse,tx0", "marvell,dove";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory {
		device_type = "memory";
		reg = <0x00000000 0x40000000>;
	};

	poweroff {
		compatible = "gpio-poweroff";
		gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
		pinctrl-0 = <&pmx_gpio_8>;
		pinctrl-names = "default";
	};
	
	gpu-subsystem {
		status = "okay";
	};
	
	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		framebuffer {
			compatible = "marvell,armada-framebuffer";
			size = <0x02000000>;
			alignment = <0x02000000>;
			no-map;
		};
	};
	
	tfp410: encoder@0 {
		compatible = "ti,tfp410";
		powerdown-gpios = <&gpio0 14 GPIO_ACTIVE_LOW>;
		ports {
			#address-cells = <1>;
			#size-cells = <0>;
			port@0 {
				reg = <0>;
				tfp410_in: endpoint@0 {
					remote-endpoint = <&dpi_out>;
				};
			};
			port@1 {
				reg = <1>;
				tfp410_out: endpoint@0 {
					remote-endpoint = <&dvi_connector_in>;
				};
			};
		};
	};

	dvi0: connector@0 {
		compatible = "dvi-connector";
		label = "dvi";
		digital;
		ddc-i2c-bus = <&i2c1>;
		hpd-gpios = <&gpio1 1 GPIO_ACTIVE_HIGH>;
		port {
			dvi_connector_in: endpoint {
				remote-endpoint = <&tfp410_out>;
			};
		};
	};
};

&eth {
	status = "okay";

	ethernet-port@0 {
		phy-handle = <&ethphy>;
	};
};

&i2c {
	pinctrl-0 = <&pmx_i2c1>;
	pinctrl-names = "default";
	clock-frequency = <100000>;
};

&i2c1 {
	status = "okay";

	ec@58 {
		compatible = "dell,wyse-ariel-ec", "ene,kb3930";
		reg = <0x58>;
	};
};

&mdio {
	status = "okay";

	ethphy: ethernet-phy@1 {
		reg = <1>;
	};
};

&pcie { status = "okay"; };

&pcie0 { status = "okay"; };

&sata0 { status = "okay"; };

&sdio1 {
	status = "okay";

	bus-width = <4>;
	//cd-gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
	//wp-gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
};

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;
	};
};

&gpu {
 status = "okay";
};

&lcd0{
	clocks = <&divider_clk 3>;   
	clock-names = "plldivider";
	status = "okay";
	
	lcd0_port: port 
	{
		dpi_out: endpoint {
			remote-endpoint = <&tfp410_in>;
		};
	};
};

&uart0 { status = "okay"; };
