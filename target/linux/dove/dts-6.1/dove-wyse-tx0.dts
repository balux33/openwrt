/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include "dove.dtsi"

/ {
	model = "Dell Wyse Tx0 (T10/T50)";
	compatible = "wyse,tx0", "marvell,dove";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory {
		device_type = "memory";
		reg = <0x00000000 0x40000000>;
	};

	poweroff {
		compatible = "gpio-poweroff";
		gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
		pinctrl-0 = <&pmx_gpio_8>;
		pinctrl-names = "default";
	};
	
	gpu-subsystem {
		status = "okay";
	};
	
	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		framebuffer {
			compatible = "marvell,armada-framebuffer";
			size = <0x02000000>;
			alignment = <0x02000000>;
			no-map;
		};
	};
	
	tfp410: encoder@0 {
		compatible = "ti,tfp410";
		powerdown-gpios = <&gpio0 14 GPIO_ACTIVE_LOW>;
		ports {
			#address-cells = <1>;
			#size-cells = <0>;
			port@0 {
				reg = <0>;
				tfp410_in: endpoint@0 {
					remote-endpoint = <&dpi_out>;
				};
			};
			port@1 {
				reg = <1>;
				tfp410_out: endpoint@0 {
					remote-endpoint = <&dvi_connector_in>;
				};
			};
		};
	};

	dvi0: connector@0 {
		compatible = "dvi-connector";
		label = "dvi";
		digital;
		ddc-i2c-bus = <&i2c1>;
		hpd-gpios = <&gpio1 1 GPIO_ACTIVE_HIGH>;
		port {
			dvi_connector_in: endpoint {
				remote-endpoint = <&tfp410_out>;
			};
		};
	};

	spdif_out: spdif-out {
		  #sound-dai-cells = <0>;
	      compatible = "linux,spdif-dit";
	};
	
	sound {
		compatible = "simple-audio-card";
		simple-audio-card,name = "Wyse_Tx0_Audio";
		simple-audio-card,mclk-fs = <256>;
 	
		simple-audio-card,dai-link@0 {		// I2S - RT5630
			reg = <0>;
			format = "i2s";
			cpu {
				sound-dai = <&audio1 0>;
			};
			codec {
				sound-dai = <&rt5630>;
			};
		};

		simple-audio-card,dai-link@1 {		// S/PDIF (line out combo jack)
			reg = <1>;
			format = "i2s";
			cpu {
				sound-dai = <&audio1 1>;
			};
			codec {
				sound-dai = <&spdif_out>;
			};
		};
	};	
};

&eth { status = "okay"; };

&ethphy {
	reg = <1>;
};

&i2c {
	pinctrl-0 = <&pmx_i2c1>;
	pinctrl-names = "default";
	clock-frequency = <100000>;
};

&i2c0 {
	rt5630: rt5630@1f {
			compatible = "realtek,rt5630";
			reg = <0x1f>;
			#sound-dai-cells = <0>;
	};
	
	idt5v49ee503: clock-generator@6A {
		#clock-cells = <0>;
		compatible = "idt,idt5v49ee503";
		reg = <0x6A>;
		factory-fout = <50000000>;
	};
};

&i2c1 {
	status = "okay";

	ec@58 {
		compatible = "dell,wyse-ariel-ec", "ene,kb3930";
		reg = <0x58>;
	};
};

&mdio { status = "okay"; };

&pcie { status = "okay"; };

&pcie0 { status = "okay"; };

&sata0 { status = "okay"; };

&sdio1 {
	status = "okay";

	bus-width = <4>;
	//cd-gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
	//wp-gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
};

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;
	};
};

&gpu {
 status = "okay";
};

&lcd0{
	//clocks = <&divider_clk 3>;   
	//clock-names = "plldivider";
	clocks = <&idt5v49ee503 0>;
	clock-names = "ext_ref_clk1";  //connected to pll out 2
	status = "okay";
	
	lcd0_port: port 
	{
		dpi_out: endpoint {
			remote-endpoint = <&tfp410_in>;
		};
	};
};

&uart0 { status = "okay"; };

&audio1 {
	status = "okay";
	#sound-dai-cells = <1>;
	clocks = <&gate_clk 13>;
	clock-names = "internal";
	pinctrl-0 = <&pmx_audio1_i2s1_spdifo>;
	pinctrl-names = "default";
};
